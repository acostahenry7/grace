<div id="services" class="{{bgImage}} services_bg_img">
  <div class="services_inf">
    <div class="container">
      <h3>{{fdTitle}}</h3>
      <div class="">
        <div class="row mt-4">
          <div class="col-md-6 p-0">
            <img class="" src="{{fdImage}}" alt="" />
          </div>
          <div class="col-md-6 text_col">
            <ul>
              {{#each fdList}}
                <li>{{item}}</li>
              {{/each}}
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div id="services_inf_light" class="services_inf_light">
    <div class="container">
      <div class="row">
        <div class="col-md-6 py-5 s_sysadmin">
          <h4>{{sdTitle}}</h4>
          <ul class="">
            {{#each sdList}}
              <li> <i class="fas fa-caret-right"></i>{{item}}</li>
            {{/each}}
          </ul>
        </div>
        <div class="col-md-6 y-aligned">
          <img src="{{sdImage}}" alt="" class="shadow rouded" />
        </div>
      </div>
    </div>
  </div>
</div>
<hr class="m-0" />
<div class="container">
  <div id="services_2" class="container services_inf_light">
    <div class="row">
      <div class="col-md-6 y_aligned">
        <img src="{{tdImage}}" alt="" class="" />
      </div>
      <div class="col-md-6 s_sysadmin" style="padding: 40px 0">
        <h4>{{tdTitle}}</h4>
        <ul>
          {{!-- En href va {{link}} --}}
          {{#each tdList}}
            <li><i class="fas fa-caret-right"></i><a
                style="text-decoration: none,  color: gray "
              >{{item}}</a></li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>